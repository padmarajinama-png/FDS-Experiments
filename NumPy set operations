import numpy as np
A=np.array([1,3,5])
B=np.array([0,2,3])
result=np.union1d(A,B)
print(result)
A=np.array([1,3,5])
B=np.array([0,2,3])
result=np.intersect1d(A,B)
print(result)
A=np.array([1,3,5])
B=np.array([0,2,3])
result=np.setdiff1d(A,B)
print(result)
A=np.array([1,3,5])
B=np.array([0,2,3])
result=np.setxor1d(A,B)
print(result)
array1=np.array([1,1,2,2,4,7,7,3,5,2,5])
result=np.unique(array1)
print(result)
